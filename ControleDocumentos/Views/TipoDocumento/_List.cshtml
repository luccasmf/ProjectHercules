@model List<ControleDocumentosLibrary.TipoDocumento>
<form action="@Url.Action("List", "TipoDocumento")" method="get" class="form-filter">
    <table id="tipoDocTable" class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Nome</th>
                <th>Ações</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var tipoDoc in Model)
            {
                <tr>
                    <td>@tipoDoc.TipoDocumento1</td>
                    <td class="center">
                        <a class="btn btn-info btnCadastro" url="@Url.Action("CarregaModalCadastro","TipoDocumento", new { idTipo = tipoDoc.IdTipoDoc })" href="#">
                            <i class="fa fa-edit"></i>
                        </a>
                        <a class="btn btn-danger btnExclusao" url="@Url.Action("CarregaModalExclusao","TipoDocumento", new { idTipo = tipoDoc.IdTipoDoc })" href="#">
                            <i class="fa fa-trash-o"></i>
                        </a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</form>
